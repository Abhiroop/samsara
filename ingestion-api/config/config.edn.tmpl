{
 :server  {:port 9000 :auto-reload false}

 :backend {:type :kafka-docker :topic "events"
           :docker {:link "kafka.*" :port "9092" :to "metadata.broker.list"} }

 :transform {:transform-fn %%TRANSFORM_FN%%
             :apply-transformation %%TRANSFORM_ENABLE%%}

 :tracking {:enabled %%TRACKING_ENABLED%% :type :riemann
            :host "%%RIEMANN_PORT_5555_TCP_ADDR%%"
            :port %%RIEMANN_PORT_5555_TCP_PORT%%
            :prefix "samsara.%%HOSTNAME%%" }

}
